{
  "identifier": "default",
  "description": "Capability for the main window",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-center",
    "core:window:allow-set-resizable",
    "core:window:allow-set-size",
    "core:window:allow-set-fullscreen",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "log:default"
  ]
}
