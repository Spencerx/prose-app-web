{
  "productName": "Prose",
  "mainBinaryName": "Prose",
  "identifier": "org.prose.app-web",
  "version": "../package.json",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3010"
  },
  "app": {
    "security": {
      "devCsp": null,
      "csp": null,
      "pattern": {
        "use": "brownfield"
      }
    },
    "windows": [
      {
        "title": "Prose",
        "width": 1360,
        "height": 940,
        "minWidth": 1080,
        "minHeight": 680,
        "resizable": true,
        "maximizable": true,
        "minimizable": true,
        "closable": true,
        "fullscreen": false,
        "center": true,
        "focus": true,
        "transparent": true,
        "visible": true,
        "dragDropEnabled": false,
        "hiddenTitle": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "publisher": "Prose Foundation",
    "category": "Business",
    "shortDescription": "Distributed, open-source, and native platform for workplace communication.",
    "createUpdaterArtifacts": "v1Compatible",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "12.0",
      "entitlements": "./entitlements.plist"
    }
  },
  "plugins": {
    "updater": {
      "endpoints": ["https://files.prose.org/apps/updates/latest.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEE5RDUzQTgxQ0U3NkQ2QgpSV1JyYmVjY3FGT2RDcG5rRG9ZZGxUeTlacFJLS0paNUZ2dkRoVFU0Tjd4UUdCMjFSckx4d1MrUAo="
    }
  }
}
